{
  "format": "POSITION_COLOR_TEX_LIGHTMAP",
  "mode": "QUADS",
  "bufferSize": "TRANSIENT",
  "sort": true,
  "affectsCrumbling": false,
  "outline": false,
  "layers": [
    {
      "type": "minecraft:texture",
      "texture": "%1$s",
      "blur": false,
      "mipmap": false
    },
    { "type": "minecraft:shader", "name": "rendertype_entity_solid" },
    {
      "type": "minecraft:lightmap"
    },
    {
      "type": "minecraft:output",
      "framebuffer": "astran:bloom"
    },
    {
      "type": "minecraft:write_mask",
      "color": true,
      "depth": true
    },
    {
      "type": "minecraft:cull",
      "face": "back"
    },
    {
      "type": "minecraft:depth_test",
      "mode": "lequal"
    },
    {
      "type": "minecraft:transparency",
      "mode": "additive"
    }
  ]
}